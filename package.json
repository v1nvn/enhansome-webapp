{
  "name": "enhansome-webapp",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "test": "npm run test:integration && npm run test:ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:ui": "vitest run --config vitest.ui.config.ts",
    "test:ui:watch": "vitest --config vitest.ui.config.ts",
    "lint": "eslint",
    "dev-check": "npm run type-check -- --incremental && npm run lint:fast -- --fix",
    "lint:fast": "eslint --cache --concurrency=auto",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && wrangler deploy --config dist/server/wrangler.json",
    "cf-typegen": "wrangler types",
    "postinstall": "npm run cf-typegen",
    "db:migrate": "wrangler d1 migrations apply enhansome-registry --local",
    "db:migrate:remote": "wrangler d1 migrations apply enhansome-registry --remote",
    "db:shell": "wrangler d1 execute enhansome-registry --local",
    "db:shell:remote": "wrangler d1 execute enhansome-registry --remote"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.13.12",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@tanstack/eslint-plugin-router": "^1.132.31",
    "@tanstack/react-devtools": "^0.7.6",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.132.47",
    "@tanstack/react-router-devtools": "^1.132.51",
    "@tanstack/react-router-ssr-query": "^1.132.47",
    "@tanstack/react-start": "^1.132.52",
    "@tanstack/react-virtual": "^3.13.12",
    "@tanstack/router-plugin": "^1.132.51",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "flexsearch": "^0.8.212",
    "globals": "^16.4.0",
    "jszip": "^3.10.1",
    "kysely": "^0.28.8",
    "kysely-d1": "^0.4.0",
    "lucide-react": "^0.545.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "typescript-eslint": "^8.46.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "0.9.12",
    "@cloudflare/workers-types": "^4.20251011.0",
    "@eslint-react/eslint-plugin": "^2.0.6",
    "@eslint/js": "^9.37.0",
    "@tanstack/eslint-config": "^0.3.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jszip": "^3.4.0",
    "@types/node": "^24.7.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "3.0.9",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vitest": "3.0.9",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.46.0"
  }
}
